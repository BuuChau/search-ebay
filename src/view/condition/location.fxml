<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="365.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="core.service.LocationService">
    <padding>
        <Insets bottom="10" left="10" right="10" top="10" />
    </padding>
    <CheckBox fx:id="cbAllLocation" layoutX="91.0" layoutY="36.0" text="All" onAction="#cbAllChecked" />
    <CheckBox fx:id="cbUSOnly" layoutX="91.0" layoutY="72.0" text="US Only" onAction="#cbUSOnlyChecked" />
    <CheckBox fx:id="cbNorthAmerica" layoutX="91.0" layoutY="108.0" text="North America" onAction="#cbNorthAmericaChecked" />
    <CheckBox fx:id="cbEurope" layoutX="91.0" layoutY="144.0" text="Europe" onAction="#cbEuropeChecked" />
    <CheckBox fx:id="cbAsia" layoutX="91.0" layoutY="180.0" text="Asia" onAction="#cbAsiaChecked" />
</AnchorPane>
